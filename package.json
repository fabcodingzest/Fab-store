{
  "name": "fabstore",
  "description": "An E-commerce site made with Next.js and Payload CMS",
  "keywords": [
    "payload",
    "cms",
    "nextjs",
    "ecommerce"
  ],
  "version": "0.0.1",
  "private": false,
  "author": "Fab",
  "scripts": {
    "lint": "next lint",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:payload": "payload build",
    "build": "NODE_ENV=production yarn build:payload && yarn build:server && NEXT_BUILD=true node dist/index.js",
    "dev": "nodemon dev.js",
    "seed": "node seed/index.js",
    "serve": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@babel/plugin-transform-modules-commonjs": "^7.14.5",
    "@babel/register": "^7.12.10",
    "apollo-link-rest": "^0.8.0-beta.0",
    "apollo-upload-client": "^16.0.0",
    "cloudinary": "^1.26.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "graphql-anywhere": "^4.2.7",
    "lodash": "^4.17.21",
    "next": "^11.1.0",
    "nodemon": "^2.0.12",
    "payload": "^0.9.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "slate": "^0.63.0",
    "slate-history": "^0.65.3",
    "slate-react": "^0.65.2",
    "streamifier": "^0.1.1",
    "stripe": "^8.171.0",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@trbl/eslint-config": "^1.2.4",
    "@types/express": "^4.17.11",
    "@types/react": "^17.0.15",
    "@types/streamifier": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-wesbos": "1.0.1",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.1",
    "ts-node": "^9.1.1"
